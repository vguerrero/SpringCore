<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
 
	<bean id="OutputHelper" class="org.vmgs.corp.output.OutputHelper">
		<property name="outputGenerator" ref="JsonOutputGenerator" />
	</bean>
 
	<bean id="CsvOutputGenerator" class="org.vmgs.corp.output.CsvOutputGenerator" />
	<bean id="JsonOutputGenerator" class="org.vmgs.corp.output.JsonOutputGenerator" />
 
</beans>